###  http://openhps.org/terms#hasAccuracy
openhps:hasAccuracy rdf:type owl:ObjectProperty ;
                    rdfs:range openhps:Accuracy ;
                    rdfs:comment "Defines the accuracy of an observation." ;
                    rdfs:label "has Accuracy"@en .


###  http://openhps.org/terms#hasOrientation
openhps:hasOrientation rdf:type owl:ObjectProperty ;
                       rdfs:domain openhps:DataObject ,
                                   openhps:Position ;
                       rdfs:range openhps:Orientation ;
                       rdfs:label "has Orientation"@en .


###  http://openhps.org/terms#hasPosition
openhps:hasPosition rdf:type owl:ObjectProperty ;
                    rdfs:domain openhps:DataObject ;
                    rdfs:range openhps:Position ;
                    rdfs:comment "The position of an object." ;
                    rdfs:label "has Position"@en .


###  http://openhps.org/terms#includesObject
openhps:includesObject rdf:type owl:ObjectProperty ;
                       rdfs:domain openhps:DataFrame ;
                       rdfs:range openhps:DataObject ;
                       rdfs:label "includes Object"@en .


###  http://openhps.org/terms#relativeTo
openhps:relativeTo rdf:type owl:ObjectProperty ;
                   rdfs:domain openhps:Position ;
                   rdfs:range openhps:DataObject ;
                   rdfs:comment "A position relative to an object or space." ;
                   rdfs:label "relative To"@en .


