    "@kevingimbel/eleventy-plugin-mermaid": "^2.2.1",
    "@quasibit/eleventy-plugin-sitemap": "^2.2.0",
    "@types/node": "^20.6.2",
    "axios": "^1.5.0",
    "chalk": "4.x",
    "decktape": "^3.10.0",
    "dotenv": "^16.3.1",
    "eleventy-favicon": "^1.1.3",
    "eleventy-plugin-html-validate": "0.0.5",
    "eleventy-plugin-pdfembed": "^1.0.2",
    "eleventy-plugin-seo": "^0.5.2",
    "eleventy-plugin-shiki-twoslash": "^1.1.3",
    "eleventy-plugin-toc": "^1.1.5",
    "eleventy-sass": "^2.2.3",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.21",
    "luxon": "^3.4.3",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.7",
    "markdown-it-attrs": "^4.1.6",
    "markdown-it-html5-media": "^0.7.1",
    "markdown-it-latex": "^0.2.0",
    "markdown-it-video": "^0.6.3",
    "netlify-cli": "^16.3.5",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.4",
    "nunjucks-markdown": "^2.0.1",
    "qrcode": "^1.5.3",
    "remark-shiki-twoslash": "^3.1.3",
    "reveal.js": "^4.6.1",
    "reveal.js-pointer": "^0.1.4",
    "rimraf": "^5.0.1",
    "serve": "^14.2.1",
    "shx": "^0.3.4",
    "unzip-stream": "^0.3.1"
