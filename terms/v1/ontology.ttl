#    Classes
#################################################################

###  http://openhps.org/terms#Absolute2DPosition
:Absolute2DPosition rdf:type owl:Class ;
                    rdfs:subClassOf :AbsolutePosition ;
                    rdfs:comment "Defines the absolute position of an object represented as a 2D point." ;
                    rdfs:label "Absolute 2D Position" .


###  http://openhps.org/terms#Absolute3DPosition
:Absolute3DPosition rdf:type owl:Class ;
                    rdfs:subClassOf :AbsolutePosition ;
                    rdfs:comment "Defines the absolute position of an object represented as a 3D point." ;
                    rdfs:label "Absolute 3D Position" .


###  http://openhps.org/terms#AbsolutePosition
:AbsolutePosition rdf:type owl:Class ;
                  rdfs:subClassOf :Position ;
                  <http://purl.org/dc/terms/source> <https://raw.githubusercontent.com/OpenHPS/openhps-core/master/src/data/position/AbsolutePosition.ts> ;
                  rdfs:comment "Defines the absolute position of an object." ;
                  rdfs:label "Absolute Position" .


###  http://openhps.org/terms#Accuracy
:Accuracy rdf:type owl:Class ;
          <http://purl.org/dc/terms/source> <https://raw.githubusercontent.com/OpenHPS/openhps-core/master/src/data/values/Accuracy.ts> ;
          rdfs:label "Accuracy" .


###  http://openhps.org/terms#AxisAngleOrientation
:AxisAngleOrientation rdf:type owl:Class ;
                      rdfs:subClassOf :Orientation ;
                      rdfs:label "Axis Angle Orientation" .


###  http://openhps.org/terms#DataFrame
:DataFrame rdf:type owl:Class ;
           rdfs:subClassOf <http://www.w3.org/ns/ssn/Input> ,
                           <http://www.w3.org/ns/ssn/Output> ;
           <http://purl.org/dc/terms/source> <https://raw.githubusercontent.com/OpenHPS/openhps-core/master/src/data/DataFrame.ts> ;
           rdfs:comment "A frame containing sensor data generated by a specific data object source." ;
           rdfs:label "Data Frame" .


###  http://openhps.org/terms#DataObject
:DataObject rdf:type owl:Class ;
            owl:equivalentClass <http://purl.oclc.org/NET/ssnx/ssn#Device> ;
            <http://purl.org/dc/terms/source> <https://raw.githubusercontent.com/OpenHPS/openhps-core/master/src/data/object/DataObject.ts> ;
            rdfs:comment "An object or person relevant for the positioning of other data objects." ;
            rdfs:label "Data Object" .


###  http://openhps.org/terms#EulerOrientation
:EulerOrientation rdf:type owl:Class ;
                  rdfs:subClassOf :Orientation ;
                  rdfs:label "Euler Orientation" .


###  http://openhps.org/terms#GeographicalPosition
:GeographicalPosition rdf:type owl:Class ;
                      owl:equivalentClass <http://www.w3.org/2003/01/geo/wgs84_pos#Point> ;
                      rdfs:subClassOf :AbsolutePosition ;
                      <http://purl.org/dc/terms/source> <https://raw.githubusercontent.com/OpenHPS/openhps-core/master/src/data/position/GeographicalPosition.ts> ;
                      rdfs:comment "Defines the absolute position of an object represented as a geographical point." ;
                      rdfs:label "Geographical Position" .


###  http://openhps.org/terms#OpenHPS
:OpenHPS rdf:type owl:Class ;
         rdfs:subClassOf <http://www.w3.org/ns/sosa/Platform> .


###  http://openhps.org/terms#Orientation
:Orientation rdf:type owl:Class ;
             owl:equivalentClass <http://purl.org/iot/vocab/m3-lite#Direction> ;
             <http://purl.org/dc/terms/source> <https://raw.githubusercontent.com/OpenHPS/openhps-core/master/src/data/position/Orientation.ts> ;
             rdfs:comment "Defines the orientation of an object." .


###  http://openhps.org/terms#Position
:Position rdf:type owl:Class ;
          rdfs:subClassOf <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ;
          <http://purl.org/dc/terms/source> <https://raw.githubusercontent.com/OpenHPS/openhps-core/master/src/data/position/Position.ts> ;
          rdfs:label "Position" .


###  http://openhps.org/terms#PositioningModel
:PositioningModel rdf:type owl:Class ;
                  rdfs:subClassOf <http://www.w3.org/ns/ssn/System> ;
                  rdfs:label "Positioning System"@en .


###  http://openhps.org/terms#QuaternionOrientation
:QuaternionOrientation rdf:type owl:Class ;
                       rdfs:subClassOf :Orientation ;
                       rdfs:label "Quaternion Orientation" .


###  http://openhps.org/terms#ReferenceSpace
:ReferenceSpace rdf:type owl:Class ;
                rdfs:subClassOf :DataObject ;
                <http://purl.org/dc/terms/source> <https://raw.githubusercontent.com/OpenHPS/openhps-core/master/src/data/object/space/ReferenceSpace.ts> ;
                rdfs:label "Reference Space" .


###  http://openhps.org/terms#RelativePosition
:RelativePosition rdf:type owl:Class ;
                  rdfs:subClassOf :Position ;
                  <http://purl.org/dc/terms/source> <https://raw.githubusercontent.com/OpenHPS/openhps-core/master/src/data/position/RelativePosition.ts> ;
                  rdfs:label "Relative Position" .


