#################################################################
#    Object Properties
#################################################################

###  http://openhps.org/terms#hasAccuracy
:hasAccuracy rdf:type owl:ObjectProperty ;
             rdfs:range :Accuracy ;
             rdfs:comment "Defines the accuracy of an observation." ;
             rdfs:label "has Accuracy"@en .


###  http://openhps.org/terms#hasOrientation
:hasOrientation rdf:type owl:ObjectProperty ;
                rdfs:domain :DataObject ,
                            :Position ;
                rdfs:range :Orientation ;
                rdfs:label "has Orientation"@en .


###  http://openhps.org/terms#hasPosition
:hasPosition rdf:type owl:ObjectProperty ;
             rdfs:domain :DataObject ;
             rdfs:range :Position ;
             rdfs:comment "The position of an object." ;
             rdfs:label "has Position"@en .


###  http://openhps.org/terms#includesObject
:includesObject rdf:type owl:ObjectProperty ;
                rdfs:domain :DataFrame ;
                rdfs:range :DataObject ;
                rdfs:label "includes Object"@en .


###  http://openhps.org/terms#relativeTo
:relativeTo rdf:type owl:ObjectProperty ;
            rdfs:domain :Position ;
            rdfs:range :DataObject ;
            rdfs:comment "A position relative to an object or space." ;
            rdfs:label "relative To"@en .


#################################################################
#    Data properties
#################################################################

###  http://openhps.org/terms#coordX
:coordX rdf:type owl:DatatypeProperty ;
        rdfs:domain :AbsolutePosition ;
        rdfs:range xsd:decimal .


###  http://openhps.org/terms#coordY
:coordY rdf:type owl:DatatypeProperty ;
        rdfs:domain :AbsolutePosition ;
        rdfs:range xsd:decimal .


###  http://openhps.org/terms#coordZ
:coordZ rdf:type owl:DatatypeProperty ;
        rdfs:domain :AbsolutePosition ;
        rdfs:range xsd:decimal .


###  http://openhps.org/terms#pitch
:pitch rdf:type owl:DatatypeProperty ;
       rdfs:domain :EulerOrientation ;
       rdfs:range xsd:decimal .


###  http://openhps.org/terms#roll
:roll rdf:type owl:DatatypeProperty ;
      rdfs:domain :EulerOrientation ;
      rdfs:range xsd:decimal .


###  http://openhps.org/terms#yaw
:yaw rdf:type owl:DatatypeProperty ;
     rdfs:domain :EulerOrientation ;
     rdfs:range xsd:decimal .


